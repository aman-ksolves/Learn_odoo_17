<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

     <record id="res_users_form_view_inherit" model="ir.ui.view">
        <field name="name">res.users.form.view.inherit</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@class='oe_title']" position="after">
                <group string="Custom Log-out">
                    <field name="enable_time_logout" string="Enable auto Logout"/>
                    <field name="max_login_time" string="Login Time in Min" invisible="not enable_time_logout" />
                </group>
            </xpath>
        </field>
    </record>


</odoo>